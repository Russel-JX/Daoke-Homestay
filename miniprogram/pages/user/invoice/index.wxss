.contain {
    padding: 0 30rpx;
}

.contain .title {
    font-family: PingFangSC-Medium;
    font-size: 50rpx;
    color: #2A2A2A;
    margin-top: 40rpx;
    font-weight: bold;
}

.contain .sel-type {
    display: flex;
    align-items: center;
    margin-top: 40rpx;
}

.contain .sel-type view {
    padding: 16rpx 20rpx;
    border: 1px solid #B4B9BF;
    border-radius: 6rpx;
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #5E646B;
}

.contain .sel-type view.sel {
    background: #e84358;
    border: 1px solid #e84358;
    color: #fff;
}

.contain .sel-type view.v2 {
    margin-left: 20rpx;
}

.contain .introduce {
    font-family: PingFangSC-Light;
    font-size: 26rpx;
    color: #B4B9BF;
    margin-top: 20rpx;
}

.contain .tit-all {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 100rpx;
}

.contain .title-two {
    font-family: PingFangSC-Medium;
    font-size: 38rpx;
    color: #2A2A2A;
    font-weight: bold;
}

.contain .list {
    margin-top: 20rpx;
}

.contain .list .list-li {
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1px solid #EBEBEB;
    padding: 40rpx 0;
}

.contain .list .list-li .list-li-l {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #5E646B;
    flex-shrink: 0;
}

.contain .list .list-li .list-li-r {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #2A2A2A;
    flex-grow: 1;
    text-align: right;
}

.contain .list .list-li .list-li-r-t {
    display: flex;
    align-items: center;
    justify-content: flex-end;
}

.contain .list .list-li .list-li-r-t view {
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1px solid #B4B9BF;
    border-radius: 4rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #5E646B;
}

.contain .list .list-li .list-li-r-t view.sel {
    background: #e84358;
    border: 1px solid #e84358;
    color: #fff;
}

.contain .list .list-li .list-li-r-t view.v2 {
    margin-left: 20rpx;
}

.contain .list .list-li .list-li-r input {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #2A2A2A;
    text-align: right;
}

.contain .list .list-li .list-li-r-tt {
    padding: 20rpx;
    box-sizing: border-box;
}

.contain .list .list-li .list-li-r textarea {
    height: 1.5em;
    width: 100%;
    padding: 0 20rpx;
}

.contain .intro-link {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #22CA92;
}

.bto-null {
    height: 250rpx;
}

.contain .bto {
    background: #FFFFFF;
    border-top: 1px solid #EBEBEB;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 150rpx;
    z-index: 10;
    display: flex;
    justify-content: center;
    align-items: center;
}

.contain .bto button {
    width: 690rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    background: #e84358;
    border-radius: 6rpx;
    font-family: PingFangSC-Regular;
    font-size: 34rpx;
    color: #fff;
}