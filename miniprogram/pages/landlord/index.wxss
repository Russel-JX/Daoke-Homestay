    text-align: left;
    border: none;
    padding: 0;
    line-height: 1;
}

.comment-score-text-bg {
    background-image: linear-gradient(to right,#FD8238 0%,#FFB869 95%);
    border-radius: 4rpx;
    color: #fff;
    font-size: 24rpx;
    padding: 0 20rpx;
    height: 40rpx;
    line-height: 40rpx;
    position: relative;
    margin-left: 12rpx;
}

.comment-score-text-bg::before {
    content: '';
    width: 0;
    height: 0;
    position: absolute;
    border: 10rpx solid transparent;
    border-right-color: #FD8238;
    left: -16rpx;
    top: 50%;
    transform: translateY(-50%);
}

.description-speek {
    font-size: 30rpx;
    color: #333;
    margin-bottom: 30rpx;
}

.description-more-popup {
