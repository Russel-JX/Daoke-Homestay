}

.description-more-text {
    line-height: 36rpx;
}

.icon-hotel-super-box {
    background-color: #fff;
    width: 36rpx;
    height: 36rpx;
    position: absolute;
    bottom: 10rpx;
    right: 0;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
    box-shadow: 0 6rpx 10rpx rgba(255,150,69,0.4);
}

.icon-hotel-super {
    width: 30rpx;
