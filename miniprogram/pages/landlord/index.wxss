    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    color: #fff;
}

.hotal-super-btm {
    width: 100%;
    position: absolute;
    height: 145rpx;
    bottom: -120rpx;
}

.isfixed {
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 1000;
}

.icon_super-comment {
    background-repeat: no-repeat;
    background-size: auto 110rpx;
}

.smrz-icon {
    width: 40rpx;
