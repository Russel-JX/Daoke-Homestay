.g-unit-sale-bar {
    display: flex;
    position: fixed;
    right: 0;
    bottom: 0;
    left: 0;
    height: 60px;
    background: #fff;
    z-index: 100;
}
.is_need_shipei {
    bottom: 68rpx;
}

.g-unit-sale-bar .icon-wrapper {
    width: 124rpx;
    padding: 29rpx 0 21rpx 0;
}

.icon-wrapper {
    background-color: #fff;
    border: none;
}

.icon {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}

.border-right {
    border-right: 2rpx solid #DADADA;
}

.icon-chat {
    width: 42rpx;
    height: 36rpx;
}

.icon-phone {
    width: 40rpx;
    height: 36rpx;
}

.desc {
    margin-top: 4rpx;
    line-height: 30rpx;
    font-size: 22rpx;
    color: #333333;
    font-weight: 300;
}

.g-unit-sale-bar .g-product {
    flex: 1;
    height: 100%;
    color: #fff;
}

.g-unit-sale-bar .g-product form {
    height: 100%;
}

.g-unit-sale-bar .g-product .go-product-inner {
    display: flex;
    padding: 0 40rpx;
    height: 100%;
    justify-content: space-between;
    align-items: center;
    border: none;
    background: #d8646f;
    border-radius: 0;
    line-height: normal;
}

.g-unit-sale-bar .g-product .go-product-inner.redbg {
    background: #ff6666;
}

.g-unit-sale-bar .g-product .go-product-inner.disabled {
    background: #dadada;
}

.g-unit-sale-bar .g-product .go-product-inner.disabled {
    background: #dadada;
    font-weight: 400;
    font-size: 34rpx;
    color: #ffffff;
    letter-spacing: 0;
    border: none;
    line-height: normal;
}

.price-des {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
}

.price-origin-final {
    display: flex;
    font-size: 40rpx;
    color: #ffffff;
}

.final,.origin {
    display: flex;
}

.final-text {
    font-weight: 500;
    font-size: 40rpx;
    color: #ffffff;
    letter-spacing: 0;
    font-weight: 800;
}

.origin {
    align-items: center;
    opacity: 0.5;
    font-size: 20rpx;
    color: #ffffff;
    letter-spacing: 0;
}

.origin-text {
    text-decoration: line-through;
    margin-bottom: -8rpx;
}

.discount {
    font-size: 20rpx;
    color: #ffffff;
    letter-spacing: 0;
}

.action {
    display: flex;
    align-items: center;
}

.action {
    font-weight: 400;
    font-size: 34rpx;
    color: #ffffff;
    letter-spacing: 0;
}

.action-order {
    display: flex;
    flex-direction: column;
    align-items: flex-end;
}

.action-order .action .quick {
    width: 24rpx;
    height: 34rpx;
}

.ml6 {
    margin-left: 6rpx;
}

.wrp1 {
    width: 100%;
}

.wrp2 {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.ml10 {
    margin-left: 10rpx;
}

.reason {
    font-size: 20rpx;
    color: #ffffff;
    letter-spacing: 0;
}

.no-price {
    width: 100%;
    text-align: center;
    font-weight: 400;
    font-size: 34rpx;
    color: #ffffff;
    letter-spacing: 0;
}

.fail {
    display: flex;
    height: 100%;
    justify-content: center;
    align-items: center;
    border: none;
    background: #fd8238;
    border-radius: 0;
    font-weight: 400;
    font-size: 34rpx;
    color: #ffffff;
    letter-spacing: 0;
}

.circular-list {
    display: flex;
    height: 100%;
    justify-content: center;
    align-items: center;
    background: #ff6666;
}

.circular {
    width: 14rpx;
    height: 14rpx;
    background-color: #fff;
    border-radius: 100%;
    margin-right: 12rpx;
    animation: load 1.4s infinite ease-in-out;
}

.circular-list view:nth-child(1) {
    animation-delay: 0.13s;
}

.circular-list view:nth-child(2) {
    animation-delay: 0.26s;
}

.circular-list view:nth-child(3) {
    animation-delay: 0.39s;
}

@-webkit-keyframes load {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}