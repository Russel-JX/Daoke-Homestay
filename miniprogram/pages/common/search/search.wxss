.scroll-fixed {
    position: fixed!important;
    left: 0;
    top: 0;
    right: 0;
    z-index: 10;
}

.scroll-y {
    height: 100%;
}

.page-search {
    height: 100%;
    box-sizing: border-box;
    position: relative;
}

.scroll-view {
    height: 100%;
}

.header-search {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    background: #fff;
    height: 88rpx;
    padding: 0 30rpx;
    position: relative;
}

.header-search__box {
    -webkit-box-flex: 1;
    flex: 1 1 auto;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    background: #f0f3f6;
    height: 56rpx;
    padding: 0 30rpx;
    border-radius: 56rpx;
}

.header-search__icon {
    display: block;
    width: 14px;
}

.header-search__input {
    width: 460rpx;
    margin-left: 20rpx;
    font-size: 26rpx;
    background: rgb(240,243,246);
    box-sizing: border-box;
}

.header-search__input__placeholder {
    font-size: 26rpx;
    color: #999;
}

.header-search__loading {
    margin-left: 20rpx;
}

.header-search__reset {
    margin-left: 20rpx;
}

.header-search__cancel {
    margin-left: 30rpx;
    font-size: 28rpx;
    color: #666;
    white-space: nowrap;
    height: 100%;
    width: 50rpx;
    display: inline-flex;
    align-items: center;
}

.list__title {
    margin: 30rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    font-weight: bold;
    color: #666;
}

.list__clear {
    float: right;
    font-weight: normal;
    font-size: 24rpx;
    color: #555555;
}

.list-columns {
    padding: 0 0 0 40rpx;
    background: #fff;
    position: relative;
    overflow: hidden;
    box-shadow: 0 1px 2px 0 rgba(0,0,0,0.08);
}

.list__item::before {
    border-bottom: 1px solid #e9e9e9;
}

.list__item {
    box-sizing: border-box;
    padding: 0 0 0 40rpx;
    font-size: 24rpx;
    line-height: 110rpx;
    height: 110rpx;
    color: #666;
    overflow: hidden;
}

.list-columns .list__item:last-child::before {
    border-bottom: none;
}

.list__item:nth-child(3n) {
    margin-right: 0;
}

.list-letter {
    overflow: hidden;
    position: fixed;
    z-index: 100;
    right: 0;
    top: 300rpx;
}

.list-letter__item {
    text-align: center;
    font-size: 22rpx;
    color: #999;
    width: 40rpx;
    line-height: 1.5;
}

.list-match .cell__bd {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    height: 48rpx;
    -webkit-box-orient: vertical;
}

.list-match__hit {
    color: #fd8238;
}

.list__noresult {
    font-size: 28rpx;
    line-height: 48rpx;
    color: #fd8238;
    font-weight: normal;
}

.swiper-tab {
    position: relative;
    height: 86rpx;
    background: #ffffff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    line-height: 86rpx;
}

.swiper-tab::after {
    position: absolute;
    left: 0;
    bottom: 0;
    content: "";
    width: 100%;
    height: 1px;
    background: #e9e9e9;
    transform: scale(1,.5);
}

.swiper-tab-item {
    -webkit-box-flex: 1;
    flex: 1 1 auto;
    text-align: center;
    font-size: 28rpx;
    color: #666;
}

.swiper-tab-item.selected {
    color: rgb(253, 62, 56);
}

.swiper-tab-item-txt {
    position: relative;
    display: inline-block;
    padding: 0 10rpx;
}

.swiper-tab-item.selected .swiper-tab-item-txt::after {
    position: absolute;
    left: 0;
    bottom: 1rpx;
    right: 0;
    content: "";
    width: 100%;
    height: 3px;
    background: rgb(253, 62, 56);
    transform: scale(1,.5);
}

.swiper-box {
    width: 100%;
    background: #ffffff;
}

.swiper-tab-content {
    height: 100%;
    width: auto;
}

.recommend-tit,.letter-tit {
    padding: 20rpx 40rpx 20rpx 60rpx;
    font-size: 28rpx;
    color: #333;
    background: #f0f3f6;
    position: relative;
    font-weight: 500;
}

.letter-tit {
    padding: 20rpx 40rpx 20rpx 40rpx;
}

.recommend-tit::after {
    content: '';
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 40rpx;
    width: 6rpx;
    height: 30rpx;
    border-radius: 40rpx;
    background-image: linear-gradient(to bottom,
    #d8646f 0%,
    #d8646f 100%);
    box-shadow: 4rpx 0 16rpx 0 rgba(255, 47, 47, 0.3);
}

.recommend-name-col {
    overflow: hidden;
    text-overflow: ellipsis;
    display: inline-block;
    margin-right: 16.6rpx;
    margin-bottom: 16.6rpx;
    width: 160rpx;
    height: 56rpx;
    line-height: 56rpx;
    background: #f3f5f8;
    font-size: 24rpx;
    color: #333333;
    border-radius: 4rpx;
    text-align: center;
    white-space: nowrap;
    padding: 0 10rpx;
    box-sizing: border-box;
    flex: none;
}

.recommend-name-col-hide {
    opacity: 0;
}

.recommend-name-col:nth-child(4n),recommend-name-col-end {
    margin-right: 0;
}

.current-name-col {
    margin-bottom: 0;
    margin: 36rpx 0 0 52rpx;
}

.search-no-result {
    display: block;
    padding: 56rpx 40rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    color: #333;
    font-weight: normal;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    background: #fff;
}

.search-no-result-icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 27rpx;
}

.scroll-history-outer {
}

.scroll-history-inner {
    padding: 30rpx 30rpx 14rpx;
    display: flex;
    flex-wrap: wrap;
    transition: height .2s;
    overflow: hidden;
    box-sizing: border-box;
    flex: none;
}

.page-suggestion {
    background: #ffffff;
}

.page-suggestion-content::before {
    border-bottom: 1px solid #e9e9e9;
}

.s-search-item {
    position: relative;
    padding: 30rpx 180rpx 30rpx 82rpx;
}

.s-search-item-line {
    position: relative;
}

.s-search-item-line:after {
    content: "";
    position: absolute;
    left: 40rpx;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    background: #e9e9e9;
    transform: scale(1,.5);
}

.search-item:last-child .s-search-item-line::after {
    background: none;
}

.s-search-right {
    position: absolute;
    right: 40rpx;
    top: 50%;
    transform: translate(0,-50%);
}

.s-search-icon {
    position: absolute;
    left: 40rpx;
    top: 50%;
    margin-top: 0;
    transform: translate(0,-50%);
    width: 32rpx;
    height: 32rpx;
    background-size: cover;
