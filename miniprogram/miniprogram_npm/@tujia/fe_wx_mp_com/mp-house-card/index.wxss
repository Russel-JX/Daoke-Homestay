}

.house-swiper-dots {
    position: absolute;
    bottom: 20rpx;
    width: 100%;
    display: flex;
    justify-content: center;
    z-index: 40;
}

.house-swiper-box {
    display: inline-flex;
    align-items: center;
}

.swiper-dots-item {
    width: 16rpx;
    height: 16rpx;
    background: #fff;
    border-radius: 100%;
    display: inline-block;
    margin: 0 6rpx;
    transition: all .2s;
    opacity: 0.8;
}

.swiper-dots-item.active {
    opacity: 1;
}