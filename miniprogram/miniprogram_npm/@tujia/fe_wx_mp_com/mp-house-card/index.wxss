.hotel-type-tag-scroll {
    width: 100%;
    white-space: nowrap;
    position: relative;
}

.tag-scroll-content {
    display: flex;
    align-items: center;
    padding-bottom: 20rpx;
}

.tag-scroll-item {
    font-size: 10px;
    color: #999;
    padding: 0 5rpx;
    height: 15px;
    line-height: 15px;
    margin-right: 15rpx;
    display: inline-flex;
    align-items: center;
    position: relative;
    box-sizing: border-box;
}

.ali-tag-item {
    display: inline-block;
    line-height: 17px;
}

.tag-scroll-content .tag-scroll-item:nth-last-child(2) {
    margin-right: 0;
}

.br1 {
    position: relative;
    box-sizing: border-box;
}

.br1::before {
    width: 200%;
    height: 200%;
    transform: scale(0.5);
    position: absolute;
    content: '';
    top: 0;
    left: 0;
    transform-origin: 0 0;
    box-sizing: border-box;
}

.tag-scroll-item::before {
    border: 1px solid #dadada;
    border-radius: 8rpx;
    box-sizing: border-box;
}

.scroll-end-mask {
    width: 94rpx;
    height: 35rpx;
    position: absolute;
    background-image: linear-gradient(to right,rgba(255,255,255,0.00) 0%,#fff 25%,#fff 100%);
    right: 0;
    top: 0;
}

.hotel-type-tag-bar {
    position: relative;
    width: 76%;
}

.tag-scroll-item-end {
    width: 94rpx;
    height: 30rpx;
    flex: none;
}

