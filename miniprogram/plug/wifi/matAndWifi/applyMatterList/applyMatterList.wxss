.g-wrapper {
    margin-top: 40rpx;
}

.dataErr {
    margin: 100rpx 0;
    color: #333;
    text-align: center;
    font-size: 28rpx;
}

.dataErr image {
    width: 180rpx;
    height: 180rpx;
    margin-bottom: 50rpx;
}

.headerInfo {
    margin: 0 40rpx;
    overflow: hidden;
}

.headerInfo .infoleft {
    float: left;
    color: #333;
    font-size: 40rpx;
}

.headerInfo .inforight {
    float: right;
    color: #999;
}

.matterlist {
    margin: 20rpx 40rpx 0 40rpx;
    padding-bottom: 100rpx;
}

.matteritem {
    margin-bottom: 40rpx;
    padding: 40rpx 40rpx 20rpx 40rpx;
    background: #fff;
    box-shadow: 0 2rpx 36rpx 0 rgba(0,0,0,0.10);
    border-radius: 12rpx;
}

.housename {
    font-weight: bold;
    margin-bottom: 20rpx;
    font-size: 32rpx;
    color: #333333;
}

.info {
    color: #999999;
    margin-bottom: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 42rpx;
}

.info .infotitle {
    float: left;
    display: inline-block;
    font-size: 28rpx;
    width: 27%;
}

.info .infoval {
    color: #333;
}

.info .infoval.limit {
    float: left;
    display: inline-block;
    width: 72%;
    vertical-align: -2rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.dateinfo {
    display: flex;
    margin-top: 40rpx;
    padding-top: 20rpx;
    vertical-align: middle;
    border-top: 1rpx solid #e9e9e9;
    font-size: 24rpx;
}

.dateinfo .enddate {
    flex: 0.5;
    color: #999;
}

.dateinfo .date {
    flex: 0.5;
    text-align: right;
    color: #dadada;
}

.bottomBtn {
    position: fixed;
    bottom: 0;
    z-index: 9;
    width: 100%;
    background: #fff;
}

.btns {
    margin: 20rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    font-size: 28rpx;
    background: #FF9645;
    border-radius: 12rpx;
}

.btnsover {
    margin: 20rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    font-size: 28rpx;
    background: #dadada;
    border-radius: 12rpx;
}